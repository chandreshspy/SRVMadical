
@{
    ViewBag.Title = "SendMessage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <div class="panel panel-default">
        <div class="table-responsive">
            <form name='frmAccount' id='frmAccount'>
                <table class="table">
                    <tr><strong>Message  Details</strong></tr>
                    <tr>
                        <td>
                            <label>Subject </label>
                        </td>
                        <td>
                            <input type="text" id="dvMainhedin" name="Mainhedin" class="editable" value="" />
                        </td>
                        <td>
                            <label>Message</label>
                        </td>
                        <td>
                            <input type="text" id="dvmainsubheding" name="mainsubheding" class="editable" value="" />
                        </td>
                        
                    </tr>
                    <tr>
                        <td>
                            <label>Enter Number With Comma Seperated</label>
                        </td>
                        <td>
                            <input type="text" id="dvwhychosfist" name="whychosfist" class="editable" value="" />
                        </td>
                        </tr>
                        
                    <tr><td class="btn btn-info btn-file"><input type="file" id="file" name="file" multiple></td></tr>
                </table>
            </form>
            <div style="margin-top: 20px;" class="col-sm-4 col-xs-12 col-lg-6 col-md-4">
                <input id="btnaboutus" class="btn btn-success" onclick="AddDetails()" type="submit" value="submit" />
            </div>
        </div>
    </div>
</div>

