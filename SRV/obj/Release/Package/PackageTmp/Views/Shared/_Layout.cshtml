<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
        });
    });
    function showhide() {
        if ($('#logmn').is(':hidden')) {
            $('#logmn').show();
            $('#oldlogmn').hide();
        } else {
            $('#logmn').hide();
            $('#oldlogmn').hide();
        }

    }
    function oldShowhide() {
        if ($('#oldlogmn').is(':hidden')) {
            $('#oldlogmn').show();
            $('#logmn').hide();
        } else {
            $('#oldlogmn').hide();
            $('#logmn').hide();
        }
    }
    var tWidth = '1050px'; // width (in pixels)
    var tHeight = '50px'; // height (in pixels)
    var tcolour = '#3c97e8'; // background colour:
    var moStop = true; // pause on mouseover (true or false)
    var fontfamily = 'arial,sans-serif'; // font for content
    var tSpeed = 3; // scroll speed (1 = slow, 5 = fast)
    // enter your ticker content here (use \/ and \' in place of / and ' respectively)
    var content = 'Customer Care Number:- +91-9310740740 Emergency  Contact:- +91 913 678 2323,+91 845 974 7454 Email:-info@srvpathlabs.com Website:-www.srvpathlab.com';

    var cps = -tSpeed;
    var aw, mq;
    var fsz = parseInt(tHeight) - 4;

    function scrollticker() {
        mq.style.left = (parseInt(mq.style.left) > (-10 - aw)) ? mq.style.left = parseInt(mq.style.left) + cps + "px" : parseInt(tWidth) + 8 + "px";
    }

    function startticker() {
        if (document.getElementById) {
            var tick = '<div style="position:relative;height:' + tHeight + ';overflow:hidden;background-color:' + tcolour + '"';
            if (moStop) tick += ' onmouseover="cps=0" onmouseout="cps=-tSpeed"';
            tick += '><strong><div id="mq" style="position:absolute;right:0px;top:0px;font-family;color: white;front-size-11;' + fontfamily + ';font-size:16px;white-space:nowrap;margin-top:1.4%;"><\/div></strong><\/div>';
            document.getElementById('ticker').innerHTML = tick;
            mq = document.getElementById("mq");
            mq.style.right = (10 + parseInt(tWidth)) + "px";
            mq.innerHTML = '<span id="tx">' + content + '<\/span>';
            aw = document.getElementById("tx").offsetWidth;
            lefttime = setInterval(scrollticker, 100);
        }
    }
    startticker();
    window.onload = startticker;
</script>